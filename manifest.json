{
    "manifest_version": 2,
    "name": "Custom New Tab",
    "version": "1.1",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "tabs"
    ]
}